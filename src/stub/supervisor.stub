[program:{{appname}}]
command=php {{appdir}}/artisan {{worker}} --queue={{queue}} --tries={{tries}} --timeout={{timeout}}
process_num={{process}}
numprocs={{process}}
process_name=%(process_num)s
priority={{priority}}
autostart=true
autorestart=unexpected
startretries={{tries}}
stopsignal=QUIT
stderr_logfile={{logfile}}